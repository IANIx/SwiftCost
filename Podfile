# platform :ios, '9.0'

target 'TestSwiftDemo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TestSwiftDemo
  pod 'SnapKit'        # 跟Masonry一样是用来设置约束的，swift版
  pod 'Alamofire'      # 用于网络请求
  pod 'Kingfisher'     # SDWebImage swift 版
  pod 'ObjectMapper'   # 字典转模型
  pod 'pop'            # facebook 的动画库
  pod 'SwiftyJSON', '~> 4.0'
  pod 'RealmSwift'     # 数据库
  pod 'GRDB.swift', '~> 5.7.4'
  pod 'JTAppleCalendar', '~> 8.0.3'
  pod 'SDMagicHook', '~> 1.2.5'
  pod 'LookinServer', :configurations => ['Debug']

end
